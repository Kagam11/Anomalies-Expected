<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	<ThingDef ParentName="BuildingBase">
		<defName>AE_BloodPump</defName>
		<label>blood-like liquid pump</label>
		<description>Collect blood-like liquid from nearby anomalous source.</description>
		<graphicData>
			<texPath>Things/Building/AE_BloodPump</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.58, 0.35, 0.25)</volume>
				<offset>(0,0,-0.1)</offset>
			</shadowData>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<tickerType>Rare</tickerType>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>7500</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>-8</Beauty>
			<Mass>26</Mass>
		</statBases>
		<fillPercent>0.15</fillPercent>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<ai_neverTrashThis>true</ai_neverTrashThis>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
			<uninstallWork>1800</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<costList>
			<Bioferrite>75</Bioferrite>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<specialDisplayRadius>6.9</specialDisplayRadius>
		<pathCost>30</pathCost>
		<passability>PassThroughOnly</passability>
		<designationCategory>Anomaly</designationCategory>
		<rotatable>false</rotatable>
		<placeWorkers>
			<li>AnomaliesExpected.PlaceWorker_ShowBloodPumpConnection</li>
		</placeWorkers>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>150</basePowerConsumption>
			</li>
			<li Class="AnomaliesExpected.CompProperties_BloodPump">
				<TickPerSpawn>60000</TickPerSpawn>
				<soundWorking>MoisturePump_Ambience</soundWorking>
			</li>
		</comps>
		<researchPrerequisites>
			<li>AEBloodPump</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</ThingDef>
	
	
	
	<ThingDef ParentName="BuildingBase">
		<defName>AE_ProviderBox</defName>
		<label>provider box</label>
		<description>Collect blood-like liquid from nearby anomalous source.</description>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
			<texPath>Things/Building/AE_ProviderBox</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.78, 0.4, 0.53)</volume>
				<offset>(0,0,0.005)</offset>
			</shadowData>
			<damageData>
				<rect>(0.11,0.24,0.78,0.53)</rect>
			</damageData>
			<!-- <drawSize>(1.28,1.28)</drawSize> -->
		</graphicData>
		<genericMarketSellable>false</genericMarketSellable>
		<tickerType>Never</tickerType>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToMake>45000</WorkToMake>
			<Flammability>0</Flammability>
			<Beauty>90</Beauty>
			<Mass>18</Mass>
		</statBases>
		<fillPercent>0.4</fillPercent>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<ai_neverTrashThis>true</ai_neverTrashThis>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
			<uninstallWork>2500</uninstallWork>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<maxItemsInCell>1</maxItemsInCell>
			<blueprintClass>Blueprint_Storage</blueprintClass>
			<storageGroupTag>ProviderBox</storageGroupTag>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<categories>
						<li>FoodRaw</li>
						<li>Textiles</li>
						<li>ResourcesRaw</li>
					</categories>
					<disallowedCategories>
						<li>EggsFertilized</li>
						<li>PlantMatter</li>
						<li>StoneBlocks</li>
					</disallowedCategories>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Preferred</priority>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<categories>
						<li>FoodRaw</li>
						<li>Textiles</li>
						<li>ResourcesRaw</li>
					</categories>
					<disallowedCategories>
						<li>EggsFertilized</li>
						<li>PlantMatter</li>
						<li>StoneBlocks</li>
					</disallowedCategories>
				</filter>
			</defaultStorageSettings>
		</building>
		<costList>
			<Bioferrite>100</Bioferrite>
			<Shard>1</Shard>
		</costList>
		<recipeMaker>
			<researchPrerequisite>AEProviderScripture</researchPrerequisite>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Artistic</workSkill>
			<unfinishedThingDef>UnfinishedSculpture</unfinishedThingDef>
			<effectWorking>Sculpt</effectWorking>
			<soundWorking>Recipe_Sculpt</soundWorking>
			<recipeUsers>
				<li>TableSculpting</li>
			</recipeUsers>
		</recipeMaker>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
		<pathCost>45</pathCost>
		<canOverlapZones>false</canOverlapZones>
		<passability>PassThroughOnly</passability>
		<surfaceType>Item</surfaceType>
		<rotatable>false</rotatable>
		<comps>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtVoidSculpture</nameMaker>
				<descriptionMaker>ArtDescription_VoidSculpture</descriptionMaker>
				<canBeEnjoyedAsArt>true</canBeEnjoyedAsArt>
			</li>
			<li Class="CompProperties_Facility">
				<maxSimultaneous>1</maxSimultaneous>
				<maxDistance>10</maxDistance>
				<statOffsets>
					<PsychicRitualQuality>0.05</PsychicRitualQuality>
				</statOffsets>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
	</ThingDef>
	
</Defs>