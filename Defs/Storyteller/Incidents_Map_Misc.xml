	<IncidentDef>
		<defName>AE_IncidentDef_BloodPuddleSpawn</defName>
		<label>strange puddle</label>
		<category>Misc</category>
		<targetTags>
			<li>Map_PlayerHome</li>
		</targetTags>
		<workerClass>AnomaliesExpected.IncidentWorker_DeployFromUnderground</workerClass>
		<letterLabel>Found strange puddle</letterLabel>
		<letterText>Your colonist noticed some small ground vibration. It seems like a pool of liquid seeping up from beneath the ground.</letterText>
		<letterDef>NeutralEvent</letterDef>
		<earliestDay>5</earliestDay>
		<baseChance>0.5</baseChance>
		<requireColonistsPresent>True</requireColonistsPresent>
		<minAnomalyThreatLevel>1</minAnomalyThreatLevel>
		<minRefireDays>10</minRefireDays>
		<modExtensions>
			<li Class="AnomaliesExpected.IncidentDefExtension">
				<DeployableObjectDef>AE_BloodPuddle</DeployableObjectDef>
				<SkyfallerDef>AE_BloodPuddleSpawner</SkyfallerDef>
				<ChanceFactorPowPerBuilding>1.1</ChanceFactorPowPerBuilding>
				<ChanceFactorPowPerOtherBuildings>
					<AE_BloodLake>
						<chance>1.5</chance>
					</AE_BloodLake>
				</ChanceFactorPowPerOtherBuildings>
			</li>
		</modExtensions>
	</IncidentDef>
  
	<IncidentDef>
